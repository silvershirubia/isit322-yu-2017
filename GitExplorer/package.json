{
  "name": "lookup-server",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "express": "4.15.3"
  },
  "scripts": {
    "start": "concurrently 'npm run server' 'npm run client' 'npm run micros'",
    "server": "babel-node server/bin/www",
    "client": "babel-node start-client.js",
    "micro-first": "babel-node ../microtest/micro-first/bin/www",
    "micro-second": "babel-node ../microtest/micro-second/bin/www",
    "qux": "babel-node ../microtest/qux/bin/www",
    "git-user": "babel-node ../microtest/git-user/bin/www",
    "gist": "babel-node ../microtest/gist/bin/www",
    "markdown": "babel-node ../microtest/markdown/bin/www",
    "git-socket": "babel-node ../microtest/git-socket/bin/www",
    "micros": "concurrently 'npm run micro-first' 'npm run micro-second' 'npm run qux' 'npm run git-user' 'npm run gist' 'npm run markdown' 'npm run git-socket' ",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "concurrently": "3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^6.10.3"
  }
}
